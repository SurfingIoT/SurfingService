<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Dsurfing.config="./config/etc/surfing" -Djava.library.path="./etc/win64" -classpath %classpath org.surfing.kernel.Kernel -classpath %classpath org.surfthing.Kernel</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>C:\Users\vinic_000\Google Drive\IoT Surfboard\apps\IoT Surfing Service</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -Dsurfing.config="./config/etc/surfing" -Djava.library.path="./etc/win64" -classpath %classpath org.surfing.kernel.Kernel -classpath %classpath org.surfthing.Kernel</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>C:\Users\vinic_000\Google Drive\IoT Surfboard\apps\IoT Surfing Service</exec.workingdir>
            </properties>
        </action>
